{
  "name": "wykra-api",
  "version": "0.0.1",
  "description": "API for third-party integrations and LLM processing",
  "author": "Wykra",
  "private": true,
  "license": "MIT License",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,db/migrations}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./db/migrations/$npm_config_name",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@langchain/anthropic": "0.3.19",
    "@langchain/core": "0.3.50",
    "@langchain/openai": "0.5.8",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.0",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/platform-express": "11.1.0",
    "@nestjs/throttler": "6.4.0",
    "@nestjs/typeorm": "11.0.0",
    "@sentry/nestjs": "9.15.0",
    "@sentry/profiling-node": "9.15.0",
    "axios": "1.9.0",
    "bull": "^4.16.5",
    "cache-manager": "6.4.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.5.0",
    "http-proxy-middleware": "^3.0.3",
    "ioredis": "5.6.1",
    "langchain": "0.3.24",
    "lodash": "4.17.21",
    "pg": "8.15.6",
    "prom-client": "^15.1.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typeorm": "0.3.19",
    "uuid": "^13.0.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.0",
    "@swc/cli": "0.7.3",
    "@swc/core": "1.11.24",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.3",
    "@types/supertest": "6.0.3",
    "@types/uuid": "^11.0.0",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "source-map-support": "0.5.21",
    "supertest": "7.1.0",
    "ts-jest": "29.3.2",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1"
  },
  "overrides": {
    "@langchain/core": "0.3.50"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@libs/entities(|/.*)$": "<rootDir>/libs/entities/src/$1",
      "^@libs/repositories(|/.*)$": "<rootDir>/libs/repositories/src/$1",
      "^@libs/exceptions(|/.*)$": "<rootDir>/libs/exceptions/src/$1",
      "^@libs/interfaces(|/.*)$": "<rootDir>/libs/interfaces/src/$1",
      "^@libs/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@libs/sentry(|/.*)$": "<rootDir>/libs/sentry/src/$1",
      "^@libs/interceptors(|/.*)$": "<rootDir>/libs/interceptors/src/$1",
      "^@libs/queue(|/.*)$": "<rootDir>/libs/queue/src/$1"
    }
  }
}
